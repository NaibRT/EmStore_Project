    @*<link href="https://fonts.googleapis.com/css?family=Lato|Open+Sans" rel="stylesheet">*@
    <link href="https://fonts.googleapis.com/css?family=Lato|Open+Sans|Poppins" rel="stylesheet">
    <link href="~/AdminContent/assets/plugins/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    @*<link href="~/AdminContent/assets/plugins/font-awesome/css/font-awesome.min.css" rel="stylesheet">*@
    <link href="~/AdminContent/assets/plugins/icomoon/style.css" rel="stylesheet">
    <link href="~/AdminContent/assets/plugins/waves/waves.min.css" rel="stylesheet">
    <link href="~/AdminContent/assets/plugins/uniform/css/default.css" rel="stylesheet">
    <link href="~/AdminContent/assets/plugins/switchery/switchery.min.css" rel="stylesheet" />
