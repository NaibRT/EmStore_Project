<body>



    <!-- Page Sidebar -->
    <div class="col-md-2" style="padding:0px!important">
        <div class="page-sidebar" style="background:transparent !important">
            <a class="logo-box" href="#">
                <span>EMsaat</span>

            </a>
            <hr />  
            <div class="page-sidebar-inner">
                <div class="page-sidebar-menu">
                    <ul class="accordion-menu">

                        <li>
                            <a href=@Url.Action("Index","AdminProduct")>
                                <span>Məhsullar</span>
                            </a>

                        </li>
                        <li>
                            <a href=@Url.Action("Index","AdminCategory")>
                                <span>Kateqoriyalar</span>
                            </a>

                        </li>
                        <li>
                            <a href=@Url.Action("Index","AdminSubCategory")>
